{
  "name": "logtint",
  "version": "0.1.0",
  "description": "Lightweight, isomorphic library that brings colors and style to your `console.log`",
  "type": "commonjs",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "scripts": {
    "postinstall": "ts-patch install",
    "build": "node buildscript/compile.mjs",
    "clean": "node buildscript/clean.mjs",
    "prepack": "npm run clean && npm run build",
    "postversion": "git add -A src && git push && git push --tags"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "ts-patch": "^3.2.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4",
    "typescript-transform-paths": "^3.5.5"
  },
  "author": "Mateusz Pietrzak",
  "license": "MIT",
  "repository": "mzpkdev/logtint",
  "keywords": [
    "isomorphic",
    "console.log",
    "console",
    "log",
    "logger",
    "logging",
    "color",
    "colour",
    "colors",
    "terminal",
    "cli",
    "tty",
    "ansi"
  ]
}
